<div class="game">
  {{#each model as |game|}}
    <h3>Matchup: {{game.home}} v {{game.away}}</h3>
    <span>Location:</span> {{game.venue}}
    <span>Time:</span> {{game.dateAndTime}}
    <span>Location:</span> {{game.city}}
    <span>Game Officials:</span> {{game.officials}}
    <span>Scratch Players:</span> {{game.scratchPlayers}}
    <div class="lineups">
      <h3>Lineups</h3>
      <span>Home Lineup {{game.homeLineup}}</span>
      <span>Home Coach {{game.homeCoach}}</span>
      <span>Away Lineup {{game.awayLineup}}</span>
      <span>Away Coach {{game.awayCoach}}</span>
    </div>
  {{/each}}
</div>
